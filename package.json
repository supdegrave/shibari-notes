    "bugs": {
        "url": "https://github.com/supdegrave/shibari-notes/issues"
    },
    "homepage": "https://github.com/supdegrave/shibari-notes#readme",
    "devDependencies": {
        "@angular/cli": "^9.0.2",
        "concurrently": "^5.1.0",
        "prettier": "1.19.1",
        "puppeteer": "^2.1.1"
    }
}
